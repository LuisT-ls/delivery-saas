{
  "buildCommand": "node vercel-build.js",
  "installCommand": "npm install --include=dev",
  "framework": "nextjs",
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "src/app/r/[restaurantId]/menu/page.tsx": {
      "maxDuration": 30
    },
    "src/app/r/[restaurantId]/item/[itemId]/page.tsx": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
